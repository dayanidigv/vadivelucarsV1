{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "lib": [
            "ES2020"
        ],
        "moduleResolution": "node",
        "types": [
            "@cloudflare/workers-types"
        ],
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true,
        "outDir": "./dist"
    },
    "include": [
        "src/**/*"
    ]
}